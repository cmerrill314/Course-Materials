<div id="main">
	<h1>WORKOUT TRACKER</h1>
	<h4>by Chris Merrill</h4>
	<h3 class="header">Add a new item below:</h1>
	<form class="form">
		<input type="hidden" name="id_input" id="id"><br>
		<label>Name:</label><input type="text" name="name" id="name"><br>
		<label>Reps:</label><input type="number" name="reps" id="reps"><br>
		<label>Weight:</label><input type="number" name="weight" id="weight"><br>
		<label>Date:</label><input type="text" name="date" id="date"><br>
		<label>lbs:</label><input type="number" name="lbs" id="lbs"><br>
		<br>
		<input type="submit" value="Add" class="submit" id="add">
	</form>
	<h3 class="header">Current Workout:</h1>
	<table id="workouts">
		<tr>
			<th>Name</th>
			<th>Reps</th>
			<th>Weight</th>
			<th>Date</th>
			<th>lbs</th>
			<th>Edit</th>
			<th>Delete</th>
		</tr>
	</table>
</div>
